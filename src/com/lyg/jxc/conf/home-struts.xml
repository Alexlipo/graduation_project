<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
	"-//Apache Software Foundation//DTD Struts Configuration 2.3//EN"
	"http://struts.apache.org/dtds/struts-2.3.dtd">

<struts>
	<package name="home-action" namespace="/" extends="base-default">
		<action name="home_*" class="com.lyg.jxc.action.HomeAction" method="{1}">
			<result name="{1}">/WEB-INF/jsp/jxc/home/{1}.jsp</result>
			<result name="home">/WEB-INF/jsp/jxc/home/home.jsp</result>
		</action>
	</package>
</struts>
